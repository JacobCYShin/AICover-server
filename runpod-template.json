{
  "name": "AICoverGen Serverless",
  "description": "AI Cover Generation with integrated UVR separation",
  "dockerImageName": "aicovergen:latest",
  "dockerArgs": "",
  "containerPort": 8000,
  "volumeMounts": [
    {
      "hostPath": "/data/aicovergen/models",
      "containerPath": "/runpod-volume/rvc_models"
    },
    {
      "hostPath": "/data/aicovergen/mdxnet",
      "containerPath": "/runpod-volume/uvr_models"
    }
